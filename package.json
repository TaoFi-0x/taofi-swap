  "scripts": {
    "compile": "npx hardhat compile",
    "test": "FORK=main npx hardhat test --deploy-fixture",
    "node": "FORK=main npx hardhat node --no-deploy",
    "deploy": "npx hardhat deploy",
    "test:mode": "FORK=mode npx hardhat test test/mode/*.spec.ts --deploy-fixture",
    "node:mode": "FORK=mode npx hardhat node --no-deploy",
    "test:linea": "FORK=linea npx hardhat test test/linea/*.spec.ts --deploy-fixture",
    "node:linea": "FORK=linea npx hardhat node --no-deploy",
    "test:sei": "FORK=sei npx hardhat test test/sei/*.spec.ts --deploy-fixture",
    "node:sei": "FORK=sei npx hardhat node --no-deploy",
    "test:op": "FORK=op npx hardhat test test/op/*.spec.ts --deploy-fixture",
    "node:op": "FORK=op npx hardhat node --no-deploy",
    "test:bittensor": "FORK=bittensor npx hardhat test test/bittensor/*.spec.ts --deploy-fixture --network localhost",
    "node:bittensor": "FORK=bittensor npx hardhat node --no-deploy",
    "debug": "FORK=main npx hardhat deployAndDebug --network localhost",
    "verify": "npx hardhat verify",
    "prepare": "husky install"
  },
